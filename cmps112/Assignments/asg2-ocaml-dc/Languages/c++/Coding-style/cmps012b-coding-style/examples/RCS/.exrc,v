@" $Id: .exrc,v 1.1 2007-07-05 18:16:40-07 - - $
"set autoindent
"set modeline
"set showmatch
map ;; 1G
map ;a :set all
map ;f {!}fmt -c -65
map ;g {!}fmt -c -50
map ;i :set ignorecase
map ;I :set noignorecase
map ;M :set noshowmatch
map ;m :set showmatch
map ;N :set nonumber
map ;n :set number
map ;s {!}sort
map ;S {!}sort -f
map ;W :set wrapmargin=0
map ;w :set wrapmargin=1
set autoprint
set ignorecase
set noslowopen
set number
set report=1
set shell=/bin/sh
set showmode
set wrapmargin=1
@
